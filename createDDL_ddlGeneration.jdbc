CREATE TABLE PROYECTO (ID INTEGER AUTO_INCREMENT NOT NULL, ACTIVO INTEGER, DESCRIPCION VARCHAR(255), NOMBRE VARCHAR(255), VERSION INTEGER, PRIMARY KEY (ID))
CREATE TABLE EMPLEADO (ID INTEGER AUTO_INCREMENT NOT NULL, DTYPE VARCHAR(31), ACTIVO INTEGER, APELLIDO1 VARCHAR(255), APELLIDO2 VARCHAR(255), CIUDAD VARCHAR(255), CP INTEGER, DIRECCION VARCHAR(255), DNI VARCHAR(255), NOMBRE VARCHAR(255), TELEFONO INTEGER, VERSION INTEGER, DEPARTAMENTO_ID INTEGER, ESFIJO TINYINT(1) default 0, TURNO INTEGER, PRIMARY KEY (ID))
CREATE TABLE DEPARTAMENTO (ID INTEGER AUTO_INCREMENT NOT NULL, ACTIVO INTEGER, CODIGO INTEGER, NOMBRE VARCHAR(255), SUELDO DOUBLE, VERSION INTEGER, PRIMARY KEY (ID))
CREATE TABLE EMPLEADO_PROYECTO (proyecto_ID INTEGER NOT NULL, empleado_ID INTEGER NOT NULL, PRIMARY KEY (proyecto_ID, empleado_ID))
ALTER TABLE EMPLEADO ADD CONSTRAINT FK_EMPLEADO_DEPARTAMENTO_ID FOREIGN KEY (DEPARTAMENTO_ID) REFERENCES DEPARTAMENTO (ID)
ALTER TABLE EMPLEADO_PROYECTO ADD CONSTRAINT FK_EMPLEADO_PROYECTO_empleado_ID FOREIGN KEY (empleado_ID) REFERENCES EMPLEADO (ID)
ALTER TABLE EMPLEADO_PROYECTO ADD CONSTRAINT FK_EMPLEADO_PROYECTO_proyecto_ID FOREIGN KEY (proyecto_ID) REFERENCES PROYECTO (ID)
